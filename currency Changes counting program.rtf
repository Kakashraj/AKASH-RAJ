#include <stdio.h>
int main()
{
 int amt;
 printf("Enter The Amount to withdrawal :");
 scanf("%d",&amt);
 printf("Required Notes of Rs 100:%d\n",(amt/100));
 printf("Required Notes of Rs 50:%d\n",(amt%100)/50);
 printf(" Required Notes of Rs 10:%d\n",((amt%100)%50)/10);
 printf("Amount Still Remaining:%d\n",((amt%100)%50)%10);
return 0;
}
}

